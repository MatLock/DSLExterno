grammar org.uqbar.paco.dsl.xtext.saludos.SaludosDSL with org.eclipse.xtext.common.Terminals

generate saludosDSL "http://www.uqbar.org/paco/dsl/xtext/saludos/SaludosDSL"

Model:
	(quienes+=Alguien)*
	(saludos+=Saludo)*;

Saludo:
	Bienvenida | Despedida;

Bienvenida:
	'Hola' aQuien=[Alguien] '!';
	
Despedida:
	'Chau' aQuien=[Alguien] '!';
	
Alguien:
	'quien' name=ID
;
	
