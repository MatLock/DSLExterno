<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="saludosDSL" nsURI="http://www.uqbar.org/paco/dsl/xtext/saludos/SaludosDSL"
    nsPrefix="saludosDSL">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="quienes" upperBound="-1"
        eType="#//Alguien" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="saludos" upperBound="-1"
        eType="#//Saludo" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Saludo">
    <eStructuralFeatures xsi:type="ecore:EReference" name="aQuien" eType="#//Alguien"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Bienvenida" eSuperTypes="#//Saludo"/>
  <eClassifiers xsi:type="ecore:EClass" name="Despedida" eSuperTypes="#//Saludo"/>
  <eClassifiers xsi:type="ecore:EClass" name="Alguien">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
